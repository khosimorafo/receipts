{
  "name": "receipts",
  "description": "A servlet to send receipts to printer or sms.",
  "version": "0.0.0",
  "author": "Khosi Morafo @khosimorafo",
  "bin": {
    "receipts": "./bin/run"
  },
  "bugs": "https://github.com/khosimorafo/receipts/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.9.7",
    "tslib": "^1",
    "twilio": "^3.42.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4.2.11",
    "@types/express": "^4.17.6",
    "@types/mocha": "^5.2.7",
    "@types/mongoose": "^5.7.10",
    "@types/node": "^10.17.19",
    "@types/supertest": "^2.0.8",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "eslint-plugin-import": "^2.20.2",
    "globby": "^10",
    "mocha": "^5.2.0",
    "nodemon": "^2.0.3",
    "nyc": "^14",
    "supertest": "^4.0.2",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/khosimorafo/receipts",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "receipts",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "khosimorafo/receipts",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "start": "tsc && nodemon ",
    "start:watch": "nodemon",
    "build": "tsc",
    "dev": "nodemon src/index.ts",
    "test-dev": "nodemon --watch . --ext ts --exec \"mocha -r ts-node/register test/**/*.test.ts\""
  },
  "types": "lib/index.d.ts"
}
